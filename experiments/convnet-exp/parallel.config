# Execution engine
exe:
  "python3"

# Script filename
script:
  "conv2d-test.py"

# CUDA devices
cuda_visible_devices:
  "1,2,3"

# Number of GPUs
n_gpus:
  3

# Number of parallel jobs per GPU
jobs_per_gpu:
  2

# Sacred experiment or not
is_sacred:
  True

# Arguments to test
args:
  #fold: [0, 1, 2]
  init_strategy: ['orthogonal', 'heuristic', 'xavier-normal', 'uninformative']
  #dataset: ['mnist', 'cifar', 'imagenet']

